input {
	rabbitmq {
		port => 5671
		ssl => true
		host => localhost
		user => "<%= @rabbitmq_username %>"
		password => "<%= @rabbitmq_password %>"
		queue => "logstash"
	}
}

# Add your filters here
filter {
	date {
		match => [ "timestamp", "UNIX_MS" ]
	}
}

output {
	elasticsearch { 
		#protocol => "http"
		bind_host => "logstash"
	}
}
